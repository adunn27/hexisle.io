{
  "name": "team-io-game",
  "version": "0.1.1",
  "author": "teamIO",
  "description": "A top down gun-action and builder webgame.",
  "license": "",
  "main": "",
  "scripts": {
    "website": "npm run build && npm run start",
    "start": "node server-bundle.js",
    "build": "npm run build-server && npm run build-client",
    "build-server": "webpack --mode development --config webpack.server.js",
    "build-client": "webpack --mode development --config webpack.client.js",
    "lint": "eslint src/ && eslint tests/",
    "lint-fix": "eslint src/ --fix && eslint tests/ --fix",
    "test": "karma start karma.conf.js --single-run"
  },
  "dependencies": {
    "express": "4.17.1",
    "http": "0.0.1-security",
    "path": "0.12.7",
    "phaser": "3.52.0",
    "socket.io": "3.1.1",
    "socket.io-client": "3.1.1"
  },
  "devDependencies": {
    "@types/express": "4.17.11",
    "@types/jasmine": "^3.6.9",
    "@types/node": "14.14.27",
    "@types/socket.io": "2.1.13",
    "@types/socket.io-client": "1.4.35",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jasmine": "^3.7.0",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.1.2",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-jasmine": "^4.0.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-typescript": "^5.5.1",
    "phantomjs-prebuilt": "^2.1.16",
    "prettier": "^2.2.1",
    "ts-loader": "8.0.17",
    "ts-node": "^7.0.1",
    "typescript": "4.1.5",
    "webpack": "5.21.2",
    "webpack-cli": "4.5.0",
    "webpack-node-externals": "2.5.2"
  }
}
